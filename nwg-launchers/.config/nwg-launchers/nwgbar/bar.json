[
  {
    "name": "_Lock screen",
    "exec": "/run/current-system/sw/bin/swaylock -f -C ~/.config/swaylock/config",
    "icon": "system-lock-screen"
  },
  {
    "name": "L_ogout",
    "exec": "swaymsg exit",
    "icon": "system-log-out"
  },
  {
    "name": "_Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "name": "_Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  }
]
