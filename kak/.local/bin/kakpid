#!/bin/sh
if [ ! -z "$KAKSESSION" ];
then
  case `uname` in
    Darwin)
      KAKPID=$(ps -ax | grep -v "grep" | grep "kak' -d -s Default" | head | grep -o '[0-9]\+' | head -1)
    ;;
    Linux)
      KAKPID=$(ps -a | grep -v "grep" | grep "kak' -d -s Default" | head | grep -o '[0-9]\+' | head -1)
      # commands for Linux go here
    ;;
    FreeBSD)
      # commands for FreeBSD go here
    ;;
  esac
  echo $KAKPID
else
echo ""
fi
